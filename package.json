{
  "name": "feathers-trigger",
  "version": "0.2.2",
  "description": "Add triggers and actions to your feathers app.",
  "author": "fratzinger",
  "homepage": "https://feathers-trigger.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/fratzinger/feathers-trigger"
  },
  "keywords": [
    "feathers",
    "feathers.js",
    "feathers-plugin",
    "trigger",
    "actions",
    "notify",
    "notification"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "dist/**"
  ],
  "scripts": {
    "build": "shx rm -rf dist/ && tsup src/index.ts --format cjs,esm --dts",
    "version": "npm run build",
    "release": "np",
    "mocha": "cross-env NODE_ENV=test TS_NODE_PROJECT='tsconfig.test.json' mocha --timeout 5000",
    "test": "npm run mocha",
    "coverage": "nyc npm run test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@feathersjs/feathers": "^4.5.11",
    "eslint-plugin-security": "^1.4.0",
    "feathers-hooks-common": "^5.0.6",
    "feathers-utils": "^1.7.2",
    "lodash": "^4.17.21",
    "object-replace-mustache": "^1.0.1",
    "sift": "^15.1.3",
    "type-fest": "^2.8.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "eslint": "^7.32.0",
    "feathers-fletching": "^1.12.2",
    "feathers-graph-populate": "^1.4.1",
    "feathers-memory": "^4.1.0",
    "mocha": "^9.1.3",
    "np": "^7.6.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "tsup": "^5.11.10",
    "typescript": "^4.5.4",
    "vuepress": "^2.0.0-beta.32"
  }
}
